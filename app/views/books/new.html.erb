<%= link_to("Home", books_path)%>
<h2> New Book </h2>
<%= form_for(@book) do |f| %>
<table>
    <tr>
        <th>Title</th>
        <td><%= f.text_field(:title)%></td>
    </tr>
    <tr>
        <th>Author</th>
        <td><%= f.text_field(:author)%></td>
    </tr>
    <tr>
        <th>Price</th>
        <td><%= f.text_field(:price)%></td>
    </tr>
    <tr>
        <th>Publish Date (yyyy-dd-yy)</th>
        <td><%= f.text_field(:publish_date)%></td>
    </tr>
</table>
<%= f.submit("Add Book")%>
<% end %>